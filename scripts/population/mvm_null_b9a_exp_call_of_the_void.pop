            {
                Target cloak_radius
                Action Enable
                Delay 0.1
            }
            OnSpawnOutput
            {
                Target cloak_prop
                Action Enable
                Delay 0.1
            }
            OnParentKilledOutput
            {
                Target cloak_stop
                Action Enable
            }
        }
