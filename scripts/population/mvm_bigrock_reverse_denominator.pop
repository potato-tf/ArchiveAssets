            {
                "targetname" "loop"
                "onspawn" "!self,trigger,0,0.1,-1"
                "ontrigger" "!parent,$weaponstripslot,4,0,-1"
                "ontrigger" "!self,trigger,0,0.1,-1"
                "spawnflags" "2"
            }
		}
