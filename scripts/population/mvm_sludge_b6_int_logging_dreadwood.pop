	PointTemplates
	{
		OnEquipMedicShotgun
		{
			OnSpawnOutput
			{
				Target !parent
				Action RunScriptCode
				Param "
					# Find and kill placeholder weapon.
					for (local i = 0; i < 7; ++i) {
						local placeholder = NetProps.GetPropEntityArray(self, `m_hMyWeapons`, i)
						if (placeholder == null || placeholder.GetName() != `tf_weapon_syringegun_medic`) continue
						placeholder.Kill()
						break
					}

					# Make a TF_WEAPON_SHOTGUN_PRIMARY
					local wep = Entities.CreateByClassname(`tf_weapon_shotgun_primary`)
					NetProps.SetPropInt(wep, `m_AttributeManager.m_Item.m_iItemDefinitionIndex`, 9)
					NetProps.SetPropBool(wep, `m_AttributeManager.m_Item.m_bInitialized`, true)
					NetProps.SetPropBool(wep, `m_bValidatedAttachedEntity`, true)
					wep.SetTeam(self.GetTeam())
					wep.DispatchSpawn()

					# Add custom attributes; original animations must be present before being granted to the player.
					wep.AddAttribute(`use original class weapon animations`, 1, -1)
					wep.AddAttribute(`max health additive penalty`, -25, -1)

					self.Weapon_Equip(wep)
					# VScript does not allow adding string attributes.
					self.AcceptInput(`$AddItemAttribute`, `custom weapon name|Medizinische Schrotflinte|TF_WEAPON_SHOTGUN_PRIMARY`, null, null)
					self.Weapon_Switch(wep)
				"
			}
		}
