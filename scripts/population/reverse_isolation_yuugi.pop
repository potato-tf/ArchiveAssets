				"tags" "no_reprogram"
			}	
			filter_multi 
			{
				"targetname" "filter_can_be_sapped"
				"Negated" "0"
				"FilterType" "0"
				"Filter01" "filter_is_red"
				"Filter02" "filter_no_tag"
				"Filter03" "filter_is_not_giant"
