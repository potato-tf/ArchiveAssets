			{
				"targetname" "reprogram_canteen_apply"
				"origin" "0 0 0"
				"mins" "-300.0 -300.0 -300.0"
				"maxs" "300.0 300.0 300.0"
				"spawnflags" "1" 
				"StartDisabled" "1"
				"filtername" "filter_can_be_sapped"

				"OnStartTouch" "!activator,$AddPlayerAttribute,receive friendly fire|1,0,-1"
