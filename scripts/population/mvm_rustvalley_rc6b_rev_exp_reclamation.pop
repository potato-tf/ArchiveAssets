			}
			OnSpawnOutput {
				Target expert_hint
				Action Kill
			}
			OnSpawnOutput {
				Target wave_start_relay_expert
				Action RunScriptCode
				Param "
					local outputs = []
					for (local i = EntityOutputs.GetNumElements(self, `OnTrigger`) - 1; i >= 0; i--)
					{
						local output = {}
						EntityOutputs.GetOutputTable(self, `OnTrigger`, output, i)
						if (endswith(output.target, `trigtimer`))
							outputs.push(output)
					}
					foreach (output in outputs)
						EntityOutputs.RemoveOutput(self, `OnTrigger`, output.target, output.input, output.parameter)
				"
			}
		}
