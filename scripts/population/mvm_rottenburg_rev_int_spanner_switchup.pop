				skin 3
			}

			prop_dynamic	// towerspawn
			{
				targetname spawnpoint_hologram
				model models/props_mvm/robot_spawnpoint.mdl
				disableshadows 1
				defaultanim idle
				origin "1534 770 -213"
				angles "0 0 0"
				skin 3
			}

			prop_dynamic	// towerspawn 2
			{
				targetname spawnpoint_hologram
				model models/props_mvm/robot_spawnpoint.mdl
				disableshadows 1
				defaultanim idle
				origin "1535 1306 -213"
				angles "0 0 0"
				skin 3
			}

			prop_dynamic	// redspawn giant
			{
				targetname spawnpoint_hologram
				model models/props_mvm/robot_spawnpoint.mdl
				disableshadows 1
				defaultanim idle
				origin "-1231 2213 -130"
