	PointTemplates
	{
		airsticky_filter //Spawn this ONCE. // Thanks from Seelpit
		{
			filter_tf_condition
			{
				"targetname" "filter_blastjump"
				"negated" "0"
				"condition" 81 //Blast jumping condition
				"OnPass" "!activator,$AddItemAttribute,fire rate bonus HIDDEN|0.35|1,0,-1"
				"OnFail" "!activator,$RemoveItemAttribute,fire rate bonus HIDDEN|1,0,-1"
			}
		}
		airsticky_equipped //Spawn this on every player with the custom weapon. // Thanks from Seelpit
		{
			OnSpawnOutput
			{
				Target "!activator"
				Action "$RemoveItemAttribute" 
				Param "fire rate bonus HIDDEN|1"
			}
			trigger_multiple
			{
				"targetname" "trigger_blastjump"
				"mins" "0 0 0"
				"maxs" "10 10 40"
				"wait" "0.1" //in case "instant" causes server lag
				"spawnflags" "64"
				"OnTrigger" "filter_blastjump,TestActivator,,0,-1"
			}
		}
	}
	
