                IncludeScript(`reskin-reanimators.nut`, getroottable())

				::CleanupPointsHUD <-
				{
					mvm_stats = Entities.FindByClassname(null, `tf_mann_vs_machine_stats`)

					function OnGameEvent_recalculate_holidays(_)
					{
						if (NetProps.GetPropInt(mvm_stats, `m_iCurrentWaveIdx`) != 0)
							return

						local obj_res = Entities.FindByClassname(null, `tf_objective_resource`)
						NetProps.SetPropInt(obj_res, `m_iNumControlPoints`, 0)

						delete ::CleanupPointsHUD
					}
				}
				__CollectGameEventCallbacks(CleanupPointsHUD)
            "
