"VertexlitGeneric"
{
	"$baseTexture" "models/props_halloween/c_fists_of_steel_purple"
	
	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" "1.0"	
	"$phongfresnelranges"	"[.25 .5 1]"
	"$basemapalphaphongmask" "1"
	"$basealphaenvmapmask" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[0.7 0.2 0.7]"
	"$lightwarptexture" "models/props_halloween/c_fists_of_steel_lightwarp_purple"
	"$phongwarptexture"  "models/props_halloween/c_fists_of_steel_phongwarp_purple"
	"$rimlight" "1"
	"$rimlightexponent" "10"	
	"$rimlightboost" ".175"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}