"VertexLitGeneric"
{
	"$basetexture" "models/weapons/c_items/c_frontierjustice"
        	"$bumpmap"    "models/weapons/c_items/c_frontierjustice_phongmask"


	"$normalmapalphaenvmapmask" 1
	"$envmap" "env_cubemap"
	"$envmaptint" "[.3 .3 .3]"

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "8"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"

	"$rimlight" "1"
	"$rimlightexponent" "10"	
	"$rimlightboost" "1.5"

	"$ambientocclusion" "1"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"weapon_invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}