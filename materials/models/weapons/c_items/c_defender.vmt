"VertexLitGeneric"
{
	"$baseTexture" "models/weapons/c_items/c_defender"
	"$envmap" "env_cubemap"
	"$envmaptint" "[.085 .085 .1]"
	"$basealphaenvmapmask"		"1"


	"$phong" "1"
	"$phongexponent" "14"
	"$phongboost" "4"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.5 .5 3]"

	"$basemapalphaphongmask" "1"

	"$rimlight" "1"			
	"$rimlightexponent" "6"	
	"$rimlightboost" "0.5"


	"$glowcolor" "1"

	"$cloakPassEnabled" "1"

	"$sheenPassEnabled" "1"

	"$sheenmap" "cubemaps/cubemap_sheen001"
	"$sheenmapmask" "effects/animatedsheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
