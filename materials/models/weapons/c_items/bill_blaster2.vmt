{
	"$baseTexture" "models/weapons/c_items/bill_blaster"

	"$phong" "1"
	"$phongexponent" "50"
	"$phongboost" "0.06"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[1.5 .5 3]"
	"halflambert" "1"
	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"
