"VertexLitGeneric"
{
	"$basetexture" "models/weapons/c_items/c_barrel_gold"

	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6
	"$yellow" "0"
	
	"$phong" "1"
	"$phongexponent" "90"
	"$phongboost" "10"
	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.5 .5 3]"
	"$halflambert" "1"

	// Begin GOLD look
	"$envmap" "cubemaps/cubemap_gold001"	
	"$envmaptint" "[2.5 2.5 1.15]"
	"$BasemapAlphaPhongMask" "1"

	// Rim lighting parameters
	"$rimlight" "1"			
	"$rimlightexponent" "50"	
	"$rimlightboost" "0"

	"360?$color2" "[ 0.5 0.5 0.5 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" "cubemaps/cubemap_sheen001"
	"$sheenmapmask" "effects/animatedsheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}


	}
}

