"VertexLitGeneric"
{
	"$baseTexture" "models\weapons\new_zealium\v_grenadelauncher"

	"$phong" "1"
               "$phongexponent" "90"
               "$phongboost" "60"
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
               "$phongfresnelranges" "[2.2 3.7 4]"
	"$rimlight" "1"						
	"$rimlightexponent" "20"				
	"$rimlightboost" ".5"
	"$basemapalphaphongmask" "1"
	// Begin GOLD look

               "$envmap" "cubemaps/cubemap_newzealium"
               $envmapfresnel 0.4
               "$envmaptint" "[ 4 4 4 ]"
	
	// End GOLD look

	
	$basealphaenvmapmask 1

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}