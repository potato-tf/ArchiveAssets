	"$bumpmap" "models/bots/demo/demo_buster_normal"
	"$selfillum" "1"
	
	"$phong" "1"
	"$phongexponent" "200"
	"$phongboost" "4"	
	"$phongfresnelranges"	"[.5 .7 1]"
	"$halflambert" "0"
