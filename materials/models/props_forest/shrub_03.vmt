"VertexlitGeneric"
{
	"$baseTexture" "models/props_forest/shrub_03"
	"$surfaceprop"	"default"
	"$alphatest" 1
	$treeSway			"1"	// height at which the effect is on fully (in world units)
	$treeSwayHeight			"0"	// radius at which the effect is on fully (in world units)
	$treeSwayStartHeight		".4"	// portion of height at which the effect starts to fade in (0-1)
	$treeSwayRadius			"0.1"	// radius at which the effect is on fully (in world units)
	$treeSwayStartRadius		"0.0"	// portion of radius at which the effect starts to fade in (0-1)
	$treeSwaySpeed			"3"	// how quickly the tree sways (as a multiplier of time)
	$treeSwayStrength		"0.01"	// how much the tree sways (as a multiplier of position)
	$treeSwayScrumbleSpeed		"1.2"	// how quickly the leaves move (as a multiplier of time)
	$treeSwayScrumbleStrength	"0.01"	// how much the leaves move( as a multiplier of position)
	$treeSwayScrumbleFrequency	"5"	// spatial frequency of sine wave applied to leaves.  Typically high-ish (10-25)
	$treeSwayFalloffExp		"6"	// falloff parameter for wave motion on branches/trunk, higher means core of tree is more stable
	$treeSwayScrumbleFalloffExp	"6"	// falloff parameter for scrumble motion on leaves, higher means the core of the tree is more stable
	$treeSwaySpeedHighWindMultiplier "0"  	// multiplier of movement speed at higher wind
	$treeSwaySpeedLerpStart 	"200.0"	// wind speed at which high wind multiplier begins to have effect
	$treeSwaySpeedLerpEnd 		"600.0"	// wind speed at which high wind multiplier is fully on

}
