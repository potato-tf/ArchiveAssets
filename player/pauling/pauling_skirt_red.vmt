"VertexLitGeneric"
{
	"$basetexture" "models/player/pauling/pauling_body_paint"
	"$bumpmap" "models/player/pauling/pauling_normal"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" 0
	"$detailblendmode" 6

	"$blendtintbybasealpha"		"1"
	//"$blendtintcoloroverbase"		"1"
	"$colortint_base"		"{47 34 34}"
	"$color2"				"{47 34 34}"
	"$colortint_tmp"		"[0 0 0]"	

	"$yellow" "0"
	"$one" "1"


	"$phong" "1"
	"$phongboost" "1"
	"$phongexponenttexture" "models/player/pauling/pauling_exponent"
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "0"

	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "2"
	"$rimmask" "1"

	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"spy_invis" //wmodel and team color cloak support
		{
		}
		"invis" //vmodel cloak support
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"		"$colortint_tmp"
			"srcVar2"		"$colortint_base"
			"resultVar"		"$color2"
		}
		"Multiply"
		{
			"srcVar1"		"$color2"
			"srcVar2"		"$yellow"
			"resultVar"		"$color2"
		}
	}
}
