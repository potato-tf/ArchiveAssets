"VertexLitGeneric"
{
	"$basetexture" "models/player/pauling/hwm/pauling_head"
	"$bumpmap" "models/player/pauling/hwm/pauling_head_nm"

	// "$stretch" "models/player/pauling/hwm/pauling_head_stretch"
	// "$compress" "models/player/pauling/hwm/pauling_head_compress"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" 0
	"$detailblendmode" 6


	"$yellow" "0"
	"$one" "1"

	"$phong" "1"
	"$phongexponenttexture" "models/player/pauling/hwm/pauling_head_exp"
	"$phongexponent" "20"

	"$phongboost" ".8"	

	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	//"$basemapalphaphongmask" "1"

	"$halflambert" "0"

	// Rim lighting parameters
	"$rimlight" "1"		
	"$rimlightexponent" "4"					
	"$rimlightboost" "2"	
//	"$rimlightexponent" "1"
//	"$rimlightboost" ".6"
	"$rimmask" "1"  	

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}

	}
}

