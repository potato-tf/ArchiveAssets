"VertexLitGeneric"
{
	"$basetexture" "models/player/pauling/pauling_hair"
	"$bumpmap" "models/player/pauling/pauling_hair_normal"

	//"$stretch" "models\player\childJack/hwm/jack_head_stretch"
	//"$compress" "models\player\childJack/hwm/jack_head_compress"


	"$envmap"	"env_cubemap"
	"$envmaptint" "[.5 .5 .5]"
	"$basetexturenoenvmap" "1"


	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" 0
	"$detailblendmode" 6

	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "50"										// Exponent for phong component of rim lighting	
	"$rimlightboost" ".4"		


	"$yellow" "0"
	"$one" "1"

	"$phong" "1"
	"$phongexponent" "40"
	"$phongboost" "1"	

	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	//"$basemapalphaphongmask" "1"

	"$halflambert" "0"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1"  "$yellow"
			"resultVar" "$color2"
		}

	}
}

