"VertexLitGeneric"
{
	"$baseTexture" "models\weapons\c_models\c_carvedcutter\c_carvedcutter"
	"$basemapalphaphongmask" 1
	"$bumpmap"	   "models\weapons\c_models\c_carvedcutter\c_carvedcutter_normal"
	"$halflambert" 1
	
	"$basemapalphaphongmask" "1"
	"$phong" "1"
	"$phongexponent" "2"
	"$phongboost" "6"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"

	"$tempvar" "0"

	"$basephongexponent" "2"
	"$basephongboost" "1"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "2"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}