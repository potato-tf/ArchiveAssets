"VertexlitGeneric"
{
	//Diffuse

	"$baseTexture" "models\weapons\c_items/c_haunted_watch"
	
	//Normale

	"$bumpmap" "models\weapons\c_items/c_haunted_watch_normal"

	//Post-Process

	"$phongwarptexture"  "models/weapons/c_items/c_haunted_watch_phongwarp"
	"$lightwarptexture" "models\weapons\c_items/c_haunted_watch_lightwarp"

	//Auto-illum du Cadran

	"$selfillum" 1
	
	//Cubemaps

	"$envmap" "env_cubemap"
	"$envmaptint" "[.5 .5 .5]"

	//Variable du phong

	"$phong" "1"

	"$phongexponent" "100"
	"$phongboost" "100"	

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"vm_invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}
